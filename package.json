{
  "name": "ejs-prerender",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "compile": "lerna run compile",
    "clean": "lerna run clean",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "update-version": "lerna publish --skip-git --skip-npm --force-publish '*'",
    "update-clean": "lerna publish",
    "check-packages": "npm clean && npm run compile && npm test && npm lint",
    "publish-packages": "./scripts/publish"
  },
  "devDependencies": {
    "lerna": "^3.19.0"
  }
}
